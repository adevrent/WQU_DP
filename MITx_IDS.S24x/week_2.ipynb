   "source": [
    "def H(q, b_1, b_2):\n",
    "    return 1 + b_1 * q**(-1) + b_2 * q**(-2)\n",
    "\n",
    "def Phi_Y(omega, b_1, b_2, sigma2):\n",
    "    return sigma2 * np.abs(H(np.exp(1j*omega), b_1, b_2))**2\n",
    "\n",
    "b_1 = 1\n",
    "b_2 = 1\n",
    "sigma2 = 1\n",
    "omega = 0\n",
    "\n",
    "Phi_Y(omega, b_1, b_2, sigma2)"
   ]
